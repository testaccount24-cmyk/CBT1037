         OPCODE 17,XR,$OPRR1,FLAGS=$OPCCL
         OPCODE 18,LR,$OPRR1
         OPCODE 19,CR,$OPRR1,FLAGS=$OPCCC
         OPCODE 1A,AR,$OPRR1,FLAGS=$OPCCA
